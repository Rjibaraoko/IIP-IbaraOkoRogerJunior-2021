<Window x:Class="WpfPriemgetal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPriemgetal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="10,0,2,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="210*"/>
            <RowDefinition Height="209*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="numbertxt" HorizontalAlignment="Left" Height="26" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="193,124,0,0"/>
        <Button x:Name="contrbtn" Content="CONTROLEER" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="193,193,0,0" Height="47" FontWeight="Bold" Grid.RowSpan="2" Click="contrbtn_Click_1"/>
        <Label x:Name="priemlbl" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="193,77,0,0" Height="37" Width="242" Grid.Row="1"/>
        <Label Content="Getal:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="118,124,0,0" Width="51"/>
        <Label Content="Priemgetal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="193,72,0,0" Height="37" Width="129" FontSize="16" FontWeight="Bold"/>

    </Grid>
</Window>
